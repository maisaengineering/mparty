<div class="col-md-6 event_right_block">
  <h4>Event Name</h4>
  <div id="large-image">
    <div class="contentplace">
      <div class="row">
        <div class="ptopbottom txt_center ptop_medium"> <span id="e_name"> Mparty</span> </div>
        <div class="content_info">
          <div class="ptopbottom">Date & Time: <span id="e_date"> 15/12/2014 </span> </div>
          <div class="ptopbottom">Location : <span id="e_locatoin"> Hyderabad</span> </div>
        </div>
      </div>
    </div>
    <div class="imageplace"></div>
  </div>
  <div class="gallery jcarousel-wrapper" id="myCarousel">
    <div class="jcarousel" data-jcarousel="false">
      <ul style="left: 0px; top: 0px;">
        <li> <a href="#"><%= image_tag('event1.png') %></a></li>
        <li><a href="#"><%= image_tag('event2.png') %></a></li>
        <li><a href="#"><%= image_tag('event3.png') %></a></li>
        <li><a href="#"><%= image_tag('event4.png') %></a></li>
        <li><a href="#"><%= image_tag('event1.png') %></a></li>
        <li><a href="#"><%= image_tag('event2.png') %></a></li>
        <li><a href="#"><%= image_tag('event3.png') %></a></li>
        <li><a href="#"><%= image_tag('event4.png') %></a></li>
      </ul>
    </div>
    <a href="#" class="jcarousel-control-prev inactive" data-jcarouselcontrol="true">‹</a>
    <a href="#" class="jcarousel-control-next" data-jcarouselcontrol="true">›</a>
  </div>
  <!--/well-->
</div>
<script type="text/javascript">
    $(document).ready(function() {
        $("#large-image .imageplace").html("<img src="+$(".jcarousel ul li ").eq(0).find('img').attr('src')+">");
        $(".jcarousel a").click(function(e){
            e.preventDefault()
            $("#large-image .imageplace").html("<img src="+$(this).find('img').attr('src')+">");

        });

        $('#myCarousel').carousel({
            interval: 1000
        })

        $('#myCarousel').on('slid.bs.carousel', function() {
            //alert("slid");
        });

    });
</script>