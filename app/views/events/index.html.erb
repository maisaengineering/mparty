<h1>All Your Events</h1>
<% if @events.present? %>
	<%= render 'shared/table', object: @events %>
<% else %>
	<h2>You don't have any Events</h2> <%= link_to 'Create Event', new_event_path %>
<% end %>
<hr/>
<h1>Invitations</h1>
<% if @invited_events.present? %>
	<%= render 'shared/table', object: @events %>
<% else %>
	<p>You don't have any invitations</p> 
<% end %>

