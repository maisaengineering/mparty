<div class="container container_stlye"><br>
  <div id="calendar"></div>
</div>
<script>
    $('#calendar').fullCalendar({
        header: {
            left: 'prev,next,today',
            center: 'title',
            right: 'month,agendaWeek,agendaDay'
        },
        eventLimit: 3,
        showTime: true,
        events: '/get_my_calendar.json',
        eventRender: function(event, element) {
            element.attr('title', event.tooltip);
            element.tooltip()
        }
    });
</script>