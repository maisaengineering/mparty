<div class="mainHeader">
  <div class="topHeader">
    <div class="headerLogo">
      <a href="/" class="logo" id="logo_top">
        <img src="/assets/maisa_logo.png">
      </a>
    </div>
    <div class="headerMenuRight">
      <%- if signed_in? %>
          <div class="headerTabs">
            <%= link_to 'Cart', spree.cart_path %>
          </div>
          <div class="headerTabs">
            <%= link_to 'My Account', spree.edit_account_path %>
          </div>
          <div class="headerTabs">
            <%= link_to "Log Out", spree.destroy_spree_user_session_path, method: :delete %>
          </div>
      <% else %>
          <%= link_to "Login", login_path %>
      <% end %>
    </div>
  </div>
  <div class="mainMenubar">
    <nav id="gallery-nav">
      <ul class="event_types">
        <li class="top"><a href="#"><span>What's Up?</span></a></li>
        <li class="top"><a href="/events"><span>Events</span></a></li>
        <li class="top"><a href="#"><span>Reviews</span></a></li>
        <li class="top"><a href="/calendar"><span>Calendar</span></a></li>
      </ul>
    </nav>
  </div>
</div>