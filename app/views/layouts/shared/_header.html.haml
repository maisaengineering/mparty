            .col-md-3
            .col-md-6
              %button.btn.btn-lg.btn-primary.btn-block{:type => "submit"} REGISTER
            .col-md-3
      .modal-footer
  / Forgot password modal box
#forgot_password_modal.modal.fade{"aria-hidden" => "true", "aria-labelledby" => "myModalLabel", :role => "dialog", :tabindex => "-1"}
  .modal-dialog
    .modal-content
      %a{:href => ""}
      %a{:href => "/"}
        %span.glyphicon.glyphicon-remove.pull-right.modal_box_x{"data-dismiss" => "modal"}
      %br/
      .modal-header
        %h3#myModalLabel.modal-title Forgot Password?
