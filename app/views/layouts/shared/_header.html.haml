.navbar-collapse.navbar-default_2.collapse
  .container
    %ul.main-nav.nav.navbar-nav
      %li
        %a{:href => "index.html"}
          %span.glyphicon.glyphicon-ok>
          \&nbsp Whats Up?
      %li
        %a{:href => "#"}
          %span.glyphicon.glyphicon-star>
          \&nbsp Events
      %li
        %a{:href => "venues.html"}
          %span.glyphicon.glyphicon-map-marker>
          \&nbsp Venues
      %li
        %a{:href => "organizers.html"}
          %span.glyphicon.glyphicon-list-alt>
          \&nbsp Organizers
      %li
        %a{:href => "mycalendar.html"}
          %span.glyphicon.glyphicon-calendar>
          \&nbsp My Calendar
  / login modal box
#login_modal.modal.fade{"aria-hidden" => "true", "aria-labelledby" => "myModalLabel", :role => "dialog", :tabindex => "-1"}
  .modal-dialog
    .modal-content
      %a{:href => ""}
      %a{:href => "/"}
        %span.glyphicon.glyphicon-remove.pull-right.modal_box_x{"data-dismiss" => "modal"}
      %br/
      .modal-header
        %a{:href => "#"}
          = image_tag('fb_icon.jpg')
        %h3#myModalLabel.modal-title Login
      .modal-body
        %form.form-signin{:role => "form"}
          %label Email Id
          %input#inputEmail.form-control{:autofocus => "", :placeholder => "Email address", :required => "", :type => "email"}/
          %br/
          %label Password
          %input#inputPassword.form-control{:placeholder => "Password", :required => "", :type => "password"}/
          %br/
          %button.btn.btn-lg.btn-primary.btn-block{:type => "submit"} Log in
          %br/
          %a{"data-dismiss" => "modal", "data-target" => "#forgot_password_modal", "data-toggle" => "modal", :href => "#"} Forgot Your Password?
          %br/
          %a.pull-right{"data-dismiss" => "modal", "data-target" => "#signup_modal", "data-toggle" => "modal", :href => "#"} REGISTER
      .modal-footer
  / SIgnUP modal box
#signup_modal.modal.fade{"aria-hidden" => "true", "aria-labelledby" => "myModalLabel", :role => "dialog", :tabindex => "-1"}
  .modal-dialog.modal2
