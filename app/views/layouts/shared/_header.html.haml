  / login modal box
#login_modal.modal.fade{"aria-hidden" => "true", "aria-labelledby" => "myModalLabel", :role => "dialog", :tabindex => "-1"}
  .modal-dialog
    .modal-content
      %a{:href => "#"}
        %span.glyphicon.glyphicon-remove.pull-right.modal_box_x{"data-dismiss" => "modal"}
      .modal-header
        %a{:href => "#"} 
          = image_tag('fb_icon.jpg',alt: 'fb login',class: 'pull-right')
        %h3.modal-title Login
      .modal-body
        %form.form-signin{:role => "form"}
          %label
          %input.form-control{:autofocus => "", :placeholder => "Email address*", :required => "", :type => "email"}/
          %label
          %input.form-control{:placeholder => "Password*", :required => "", :type => "password"}/
          %label
          %button.btn.btn-lg.btn-primary.btn-block{:type => "submit"} Log in
          %label
          .row.no_margin
            %a{"data-dismiss" => "modal", "data-target" => "#forgot_password_modal", "data-toggle" => "modal", :href => "#"} Forgot Your Password?
            %a.pull-right{"data-dismiss" => "modal", "data-target" => "#signup_modal", "data-toggle" => "modal", :href => "#"} REGISTER
      .modal-footer

  / SIgnUP modal box
#signup_modal.modal.fade{"aria-hidden" => "true", "aria-labelledby" => "myModalLabel", :role => "dialog", :tabindex => "-1"}
  .modal-dialog.modal2
    .modal-content
      %a{:href => ""}
        %span.glyphicon.glyphicon-remove.pull-right.modal_box_x{"data-dismiss" => "modal"}
      .modal-header
        %h3#myModalLabel.modal-title Create New Account
      .modal-body
        %form.form-signin{:role => "form"}
          .row
            .col-md-6
              %input.form-control{:autofocus => "", :placeholder => "First Name*", :required => "", :type => "text"}/
            .col-md-6
              %input.form-control{:placeholder => "Last Name*", :required => "", :type => "text"}/
          / <span class="err"> error occured!</span>
          %br/
          .row
            .col-md-12
              %input.form-control{:placeholder => "Email Address*", :required => "", :type => "email"}/
          %br/
          .row
            .col-md-6
              %input.form-control{:autofocus => "", :placeholder => "Password*", :required => "", :type => "password"}/
            .col-md-6
              %input.form-control{:placeholder => "Password Confirmation*", :required => "", :type => "password"}/
          %br/
          .row
            .col-md-12
              %input.form-control{:placeholder => "Phone Number*", :required => "", :type => "tel"}/
          %br/
          .row
            .col-md-3
            .col-md-6
              %button.btn.btn-lg.btn-primary.btn-block{:type => "submit"} REGISTER
            .col-md-3
      .modal-footer

  / Forgot password modal box
#forgot_password_modal.modal.fade{"aria-hidden" => "true", "aria-labelledby" => "myModalLabel", :role => "dialog", :tabindex => "-1"}
  .modal-dialog
    .modal-content
      %a{:href => "#"}
        %span.glyphicon.glyphicon-remove.pull-right.modal_box_x{"data-dismiss" => "modal"}
      .modal-header
        %h3.modal-title Forgot Password?
      .modal-body
        %form.form-signin{:role => "form"}
          %label
          %input.form-control{:autofocus => "", :placeholder => "Enter Your Email Id*", :required => "", :type => "email"}/
          %label
          %button.btn.btn-lg.btn-primary.btn-block{:type => "submit"} SEND
      .modal-footer

