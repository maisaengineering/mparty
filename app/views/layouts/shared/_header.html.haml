          %label Enter Your Email Id
          %input#inputEmail.form-control{:autofocus => "", :placeholder => "Email address", :required => "", :type => "email"}/
          %br/
          %button.btn.btn-lg.btn-primary.btn-block{:type => "submit"} SEND
      .modal-footer
