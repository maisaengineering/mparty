              %input#inputPassword.form-control{:placeholder => "", :required => "", :type => "text"}/
          %br/
          .row
            .col-md-12
              %label Email Address*
              %input#inputPassword.form-control{:placeholder => "", :required => "", :type => "email"}/
          %br/
          .row
            .col-md-6
              %label Password*
              %input#inputEmail.form-control{:autofocus => "", :placeholder => "", :required => "", :type => "password"}/
            .col-md-6
              %label Password Confirmation*
              %input#inputPassword.form-control{:placeholder => "", :required => "", :type => "password"}/
          %br/
          .row
            .col-md-12
              %label Email Address*
              %input#inputPassword.form-control{:placeholder => "", :required => "", :type => "email"}/
          %br/
