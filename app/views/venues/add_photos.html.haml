.container
  .create_event
    .row
      %h2.event_details Upload Venue photos
    #pictures
      #pictures
        - unless @venue.pictures.blank?
          - @venue.pictures.each do |picture|
            = render 'pictures/picture',picture: picture
      = form_for Picture.new do |f|
        = f.label :image, "Upload pictures:"
        = hidden_field_tag :resource_id ,@venue.id
        = hidden_field_tag :resource ,'venue'
        = f.file_field :image,multiple: true, name: "picture[image]"