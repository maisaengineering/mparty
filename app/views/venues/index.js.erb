<% if params[:query].present? %>
$('#venue_filter_results').html('<%= j(render 'venues',venues: @venues)   %>');
$('#venues_result_count').text('<%= @venues.size %>')
<% else %>
    $('#all_venues').append('<%= j(render 'venues',venues: @venues)   %>');
    <% if @venues.next_page %>
        $('.pagination').replaceWith('<%= j(paginate @venues ,params: {query: params[:query] ,venue_id: params[:venue_id],from_event: params[:from_event]}) %>');
        <% else %>
        $('.pagination').remove();
    <% end %>
<% end  %>

$('div.rateit').rateit();
$('[data-toggle="tooltip"]').tooltip()